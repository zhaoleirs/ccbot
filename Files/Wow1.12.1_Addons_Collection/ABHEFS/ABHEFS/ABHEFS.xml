					<Anchor point="TOPLEFT" relativeTo="ABHEFS_HordeFlag2" relativePoint="BOTTOMLEFT"/>
				</Anchors>
			</Frame>
			<Frame name="ABHEFS_HordeFlag4" inherits="ABHEFS_HordeFlagTemplate">
				<Anchors>
