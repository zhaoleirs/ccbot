<Bindings>
    <Binding name="AAACTION" description="Perform combat action." header="AA">
        AutoAttack_Action()
    </Binding>
    <Binding name="AAINSTANT" description="Instant combat action.">
        AutoAttack_Action(1)
    </Binding>
    <Binding name="AATPT" description="Target Previous Target.">
        TargetLastTarget()
    </Binding>
    <Binding name="AAMOUNT" description="Switch mount.">
        AutoAttack_Switch('mount')
    </Binding>
    <Binding name="AAFORM" description="Switch form.">
        AutoAttack_Switch('form')
    </Binding>
</Bindings>
