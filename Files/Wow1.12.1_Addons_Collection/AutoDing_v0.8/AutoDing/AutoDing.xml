<!--

	UniAutoDing: AutoDings to whatever channel you want.
	/autoding or /ad for help

	Unique of Dalaran & Lightning Troopers guild.

-->
<Ui xmlns="http://www.blizzard.com/wow/ui/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.blizzard.com/wow/ui/
..\FrameXML\UI.xsd">
	<Script file="AutoDing.lua"/>
	<Frame name="AutoDingFrame" hidden="true">
		<Scripts>
			<OnLoad>
				UniAutoDing_OnLoad();
			</OnLoad>
			<OnEvent>
				UniAutoDing_OnEvent();
			</OnEvent>
			<OnUpdate>
				UniAutoDing_OnUpdate();
			</OnUpdate>
		</Scripts>
	</Frame>
</Ui>
