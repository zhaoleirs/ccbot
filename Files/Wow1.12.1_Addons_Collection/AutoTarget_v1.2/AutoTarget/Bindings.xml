<Bindings>
    <Binding name="AUTOTARGETSELECT" description="Toggle on/off" header="AUTOTARGET">
        AutoTarget_command('');
    </Binding>
    <Binding name="AUTOTARGETCONFIG" description="Toggle config. window">
        AutoTarget_Toggle_Config();
    </Binding>
    <Binding name="AUTOTARGETTARGET" description="Target current target's target">
        TargetUnit('targettarget');
    </Binding>
    <Binding name="AUTOTARGETCLEAR" description="Clear the current target">
        ClearTarget();
    </Binding>
</Bindings>
