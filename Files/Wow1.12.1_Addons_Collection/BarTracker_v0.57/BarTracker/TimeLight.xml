<Ui xmlns="http://www.blizzard.com/wow/ui/"   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"   xsi:schemaLocation="http://www.blizzard.com/wow/ui/">

	<Script file="TimeLight.lua"/>

	<!-- Frame to handle all core events -->
	<Frame name="TimeLight_MainFrame" parent="UIParent">
		<Scripts>
			<OnLoad>
				TimeLight_OnLoad();
			</OnLoad>
			<OnEvent>
				TimeLight_OnEvent();
			</OnEvent>
			<OnUpdate>
				TimeLight_OnUpdate();
			</OnUpdate>
		</Scripts>
	</Frame>

</Ui>

