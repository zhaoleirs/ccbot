	<Script file="BibActionButtons.lua"/>
	<CheckButton name="BibActionBarButtonTemplate" inherits="ActionBarButtonTemplate" virtual="true">
		<Layers>
			<Layer><Texture name="$parentCover" file="Interface\AddOns\BibToolbars\BibButtonCover"/></Layer>
		</Layers>
		<Scripts>
			<OnLoad>
				--Register for default events
				this:RegisterEvent("VARIABLES_LOADED");
				ActionButton_OnLoad();
				
				--Initialize the button with the default IDs
				this.originalID = this:GetID();
				SetBibButtonIDDefaults(this);
				this.currentID = this.normalID;
				BibUpdateRegisteredEvents(this);
				
				--Set this button's binding name
				local name = this:GetName();
				if(12 >= this.normalID) then
					this.bindingName = "ACTIONBUTTON"..string.sub(name, string.len("BibActionButton") + 1, string.len(name) + 1);
				else
					this.bindingName = "BIBACTIONBUTTON"..string.sub(name, string.len("BibActionButton") + 1, string.len(name) + 1);
				end
				
				local cover = getglobal(this:GetName().."Cover");
				cover:SetVertexColor(0.0, 0.0, 0.0, 0.0);
				this.showgrid = 1;
				InitializeButtonIDs();
			</OnLoad>
			<OnEvent>
				if(event == "VARIABLES_LOADED") then				
					if(GetHashedSaveValue("NormalIDs", this:GetName()) == nil) then
						CopyButtonIDsToSaveRegisters(this);
					else
						CopySaveRegistersToButtonIDs(this);
					end	
					return;
				end
				
				if (event == "VARIABLES_LOADED" or
				    event == "UPDATE_BINDINGS") then
				    BibUpdateActionButtonMapText();
				end
				
				if (event == "ACTIONBAR_PAGE_CHANGED" or
				   event == "PLAYER_ENTER_COMBAT" or
				   event == "PLAYER_LEAVE_COMBAT" or
				   event == "UPDATE_BONUS_ACTIONBAR" or
				   event == "PLAYER_COMBO_POINTS") then
				   	if (event == "PLAYER_ENTER_COMBAT") then
						this.incombat = true;
					elseif (event == "PLAYER_LEAVE_COMBAT") then
						this.incombat = false;
					end
					UpdateBibButtonID(this);
					ActionButton_UpdateState();
				end
			
				if ( not (event == "UPDATE_BINDINGS") ) then
					ActionButton_OnEvent(event);
				end
			</OnEvent>
			<OnClick>
				if(IsControlKeyDown() and not BibActionBarButtonsLocked) then
					PopupBibButtonIdMenu();
					ActionButton_UpdateState();
					return;
				end

				if ( IsShiftKeyDown() ) then
					PickupAction(ActionButton_GetPagedID(this));
				else
					if ( MacroFrame_SaveMacro ) then
						MacroFrame_SaveMacro();
					end
					UseAction(ActionButton_GetPagedID(this), 1);
				end
				ActionButton_UpdateState();
			</OnClick>
			<OnDragStart>
				if(not BibActionBarButtonsLocked) then
					PickupAction(ActionButton_GetPagedID(this));
					ActionButton_UpdateState();
				end
			</OnDragStart>
			<OnUpdate>
				if(this.backupID ~= nil) then
					local shouldDisplayBackupID = ShouldDisplayBackupID(this);
					if(shouldDisplayBackupID ~= this.shouldDisplayBackupID) then
						UpdateBibButtonID(this);
						this.shouldDisplayBackupID = shouldDisplayBackupID;
					end
				end
				BibActionButton_UpdateCover();
				ActionButton_OnUpdate(arg1);
			</OnUpdate>
		</Scripts>
	</CheckButton>
	
