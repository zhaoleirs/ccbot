<Bindings>
  <Binding name="TOGGLEBIGMINIMAP"
	   description="Toggle Minimap Size"
	   header="BIGMINIMAP">
    BigMinimap_ToggleSize();
  </Binding>

  <Binding name="TEMPBIGMINIMAP"
	   description="Toggle Minimap Temporarily"
	   runOnUp="true">
    BigMinimap_ToggleSize();
  </Binding>
</Bindings>