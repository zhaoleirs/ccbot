<!-- CashNotify - v1.31.11200 -->
<!-- Last Update: 23 oct 2006 -->

<!-- User interface key bindings -->
<Bindings>
	<!-- Enable notifications -->
	<Binding name="CASHNOTIFY_ENABLE" header="CASHNOTIFY_HEADER">
		CashNotify_SetState(true);
	</Binding>

	<!-- Disable notifications -->
	<Binding name="CASHNOTIFY_DISABLE">
		CashNotify_SetState(false);
	</Binding>

	<!-- Toggle notification state -->
	<Binding name="CASHNOTIFY_TOGGLE">
		CashNotify_ToggleState();
	</Binding>

	<!-- Toggle merchant summaries -->
	<Binding name="CASHNOTIFY_TOGMERCH">
		CashNotify_ToggleMerchantSummaries();
	</Binding>

	<!-- Toggle output frame -->
	<Binding name="CASHNOTIFY_TOGFRAME">
		CashNotify_ToggleFrame();
	</Binding>
</Bindings>
