            <OnLoad>
                CharactersViewer.PaperDollFrame.OnLoad();
            </OnLoad>
            <OnShow>
                CharactersViewer.PaperDollFrame.OnShow();
            </OnShow>
				<OnHide>
					if ( CVCharacterFrame.isMoving ) then
						CVCharacterFrame:StopMovingOrSizing();
						CVCharacterFrame.isMoving = false;
					end
					CharactersViewer.PaperDollFrame.OnHide();
				</OnHide>
				<OnMouseUp>
					if ( CVCharacterFrame.isMoving ) then
						CVCharacterFrame:StopMovingOrSizing();
						CVCharacterFrame.isMoving = false;
					end
				</OnMouseUp>
				<OnMouseDown>
					if ( ( ( not CVCharacterFrame.isLocked ) or ( CVCharacterFrame.isLocked == 0 ) ) and ( arg1 == "LeftButton" ) ) then
						CVCharacterFrame:StartMoving();
						CVCharacterFrame.isMoving = true;
					end
				</OnMouseDown>        
		  </Scripts>
