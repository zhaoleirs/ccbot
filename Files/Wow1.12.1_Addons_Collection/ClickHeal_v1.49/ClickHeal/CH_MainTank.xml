<Ui xmlns="http://www.blizzard.com/wow/ui/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.blizzard.com/wow/ui/">

	<!-- ===== Main tank anchor ============================================================================== -->

	<Frame name="CH_MTAnchor" inherits="CH_AnchorTemplate" />
	<Frame name="CH_MTBottomAnchor" inherits="CH_AnchorBottomTemplate" />

	<!-- ===== Main Tanks ================================================================================== -->

	<Button name="CH_MT1Frame" inherits="CH_UnitFrameTemplate">
		<Anchors>
			<Anchor point="TOP" relativeTo="CH_MTAnchor" relativePoint="BOTTOM" />
		</Anchors>
		<Scripts>
			<OnLoad>
				CH_OnUnitFrameLoad(nil,'MT1',nil);
			</OnLoad>
			<OnUpdate>
				CH_OnUnitFrameUpdate(this.index,this.frameUnit,this.frameData,false,false,false,true,false);
			</OnUpdate>
		</Scripts>
	</Button>

