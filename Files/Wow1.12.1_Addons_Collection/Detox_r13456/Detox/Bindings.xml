<Bindings>
	<Binding name="CLEAN" header="DETOX">
			Detox:Clean()
	</Binding>
	<Binding name="PRIORITYTOGGLE">
			Detox:PriorityToggle()
	</Binding>
	<Binding name="PRIORITYTOGGLEGROUP">
			Detox:PriorityToggle("group")
	</Binding>
	<Binding name="PRIORITYTOGGLECLASS">
			Detox:PriorityToggle("class")
	</Binding>
</Bindings>
