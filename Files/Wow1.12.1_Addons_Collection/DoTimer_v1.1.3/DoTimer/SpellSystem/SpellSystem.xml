<Ui xmlns="http://www.blizzard.com/wow/ui/"   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"   xsi:schemaLocation="http://www.blizzard.com/wow/ui/">
	<Script file="SpellSystem.lua"></Script>
	<Frame name="SpellSystemFrame">
		<Scripts>
			<OnLoad> SpellSystem_OnLoad(); </OnLoad>
			<OnEvent> SpellSystem_OnEvent(event); </OnEvent>
		</Scripts>
	</Frame>
	<Frame name="SpellSystemDelayFrame"/>
	<Frame name="SpellSystemPreTimerFrame"/>
	<Frame name="SpellSystemNeedTargetFrame"/>
	<GameTooltip name="SpellSystemScanningFrame" inherits="GameTooltipTemplate">
		<Scripts>
			<OnLoad> this:SetOwner(this,"ANCHOR_NONE"); </OnLoad>
		</Scripts>
	</GameTooltip>
</Ui>
