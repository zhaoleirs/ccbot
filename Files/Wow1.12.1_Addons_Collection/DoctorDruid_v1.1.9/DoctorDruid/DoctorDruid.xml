<Frame name="DoctorDruid_HealStats" inherits="DDSF" toplevel="true" parent="UIParent" frameStrata="DIALOG" hidden="true" enableMouse="true">
	<Scripts><OnShow>DD__Setupwindow(8)</OnShow></Scripts>
	<Size><AbsDimension x="480" y="430"/></Size>
	<Frames>

	<Frame name="$parent_MainFrame" inherits="OptionFrameBoxTemplate">
		<Size>
			<AbsDimension x="440" y="365"/>
		</Size>
		<Anchors>
			<Anchor point="TOPLEFT">
				<Offset>
					<AbsDimension x="20" y="-25"/>
				</Offset>
			</Anchor>
		</Anchors>
		<Scripts>
			<OnLoad>
				this:SetBackdropBorderColor(0.4, 0.4, 0.4);
				this:SetBackdropColor(0.15, 0.15, 0.15);
			</OnLoad>
		</Scripts>
		<Layers><Layer level="ARTWORK">
			<FontString name="$parent_Evaluated" inherits="GameFontNormalSmall" justifyH="LEFT" text="">
				<Size><AbsDimension x="800"/></Size>
				<Anchors><Anchor point="TOPLEFT" relativeTo="$parent" relativePoint="TOPLEFT">
						<Offset><AbsDimension x="25" y="-65"/></Offset>
				</Anchor></Anchors>					
			</FontString>
			<FontString name="$parent_Evaluated2" inherits="GameFontNormalSmall" justifyH="RIGHT" text="">
				<Size><AbsDimension x="800"/></Size>
				<Anchors><Anchor point="TOPRIGHT" relativeTo="$parent" relativePoint="TOPRIGHT">
						<Offset><AbsDimension x="-85" y="-65"/></Offset>
				</Anchor></Anchors>					
			</FontString>
		</Layer></Layers>
		<Frames>

			<Frame name="$parent_BasicFrameTitle" inherits="DDTB">
				<Scripts><OnShow>DD__SetText(138)</OnShow></Scripts>
				<Anchors><Anchor point="TOP"><Offset><AbsDimension x="0" y="-10"/></Offset></Anchor></Anchors>			
			</Frame>

			<EditBox name="$parent_HealBonus" inherits="DoctorDruidEditBoxTemplate" letters="5">
				<Size><AbsDimension x="60" y="20"/></Size>
				<Anchors>
					<Anchor point="TOPLEFT" relativeTo="$parent" relativePoint="TOPLEFT">
					<Offset><AbsDimension x="25" y="-35"/></Offset></Anchor>
				</Anchors>
				<Scripts>
					<OnLoad>
						this:SetNumber(0);
						if BonusScanner then if BonusScanner.bonuses["HEAL"] then
							this:SetNumber(BonusScanner.bonuses["HEAL"]);
						end end
						DoctorDruid_HealStats_MainFrame_HealBonus_Description:SetText(DOCDRUID[139]);
					</OnLoad>
				</Scripts>
			</EditBox>
			<EditBox name="$parent_Mana" inherits="DoctorDruidEditBoxTemplate" letters="5">
				<Size><AbsDimension x="60" y="20"/></Size>
				<Anchors>
					<Anchor point="TOPLEFT" relativeTo="$parent" relativePoint="TOPLEFT">
					<Offset><AbsDimension x="160" y="-35"/></Offset></Anchor>
				</Anchors>
				<Scripts>
					<OnLoad>
						this:SetNumber(UnitManaMax("player"));
						DoctorDruid_HealStats_MainFrame_Mana_Description:SetText(DOCDRUID[140]);
					</OnLoad>
				</Scripts>
			</EditBox>
			<EditBox name="$parent_Manareg" inherits="DoctorDruidEditBoxTemplate" letters="5">
				<Size><AbsDimension x="60" y="20"/></Size>
				<Anchors>
					<Anchor point="TOPLEFT" relativeTo="$parent" relativePoint="TOPLEFT">
					<Offset><AbsDimension x="295" y="-35"/></Offset></Anchor>
				</Anchors>
				<Scripts>
					<OnLoad>
						this:SetNumber(0);
						if BonusScanner then if BonusScanner.bonuses["MANAREG"] then
							this:SetNumber(BonusScanner.bonuses["MANAREG"]);
						end end
						DoctorDruid_HealStats_MainFrame_Manareg_Description:SetText(DOCDRUID[141]);
					</OnLoad>
				</Scripts>
			</EditBox>
		</Frames>
	</Frame>

	<Button name="$parent_Button1" inherits="DDBT">
		<Anchors><Anchor point="BOTTOMLEFT"><Offset><AbsDimension x="20" y="16"/></Offset></Anchor></Anchors>
		<Scripts>
			<OnShow>DD__SetText(142)</OnShow>
			<OnClick>
				DDui_Heal = DoctorDruid_HealStats_MainFrame_HealBonus:GetNumber(); if not DDui_Heal then DDui_Heal = 0; end
				DDui_Mana = DoctorDruid_HealStats_MainFrame_Mana:GetNumber(); if not DDui_Mana then DDui_Mana = 0; end
				DDui_Mreg = DoctorDruid_HealStats_MainFrame_Manareg:GetNumber(); if not DDui_Mreg then DDui_Mreg = 0; end
				stats = ddheal(DDui_Heal,DDui_Mana,DDui_Mreg);
				DoctorDruid_HealStats_MainFrame_Evaluated:SetText(stats[1]);
				DoctorDruid_HealStats_MainFrame_Evaluated2:SetText(stats[2]);
			</OnClick>
		</Scripts>
	</Button>

	<Button name="$parent_Button2" inherits="DDBT">
		<Anchors><Anchor point="BOTTOMRIGHT"><Offset><AbsDimension x="-170" y="16"/></Offset></Anchor></Anchors>
		<Scripts>
			<OnShow>DD__SetText(143)</OnShow>
			<OnClick>
				if BonusScanner then _temp = BonusScanner.bonuses["HEAL"]; if not _temp then _temp = 0; end DoctorDruid_HealStats_MainFrame_HealBonus:SetNumber(_temp); end
				_temp = UnitManaMax("player"); if not _temp then _temp = 0; end DoctorDruid_HealStats_MainFrame_Mana:SetNumber(_temp);
				if BonusScanner then _temp = BonusScanner.bonuses["MANAREG"]; if not _temp then _temp = 0; end DoctorDruid_HealStats_MainFrame_Manareg:SetNumber(_temp); end
			</OnClick>
		</Scripts>
	</Button>
	<Button name="$parent_Button3" inherits="DDBT">
		<Anchors><Anchor point="BOTTOMRIGHT"><Offset><AbsDimension x="-20" y="16"/></Offset></Anchor></Anchors>
		<Scripts><OnClick>DD__Setupwindow(1)</OnClick><OnShow>DD__SetText(192)</OnShow></Scripts>
	</Button>

	</Frames>
</Frame>

