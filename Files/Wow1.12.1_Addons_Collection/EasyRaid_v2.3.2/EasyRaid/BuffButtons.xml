	<Frame name="ER_RaidBuffButtonTemplate" inherits="PartyBuffButtonTemplate" virtual="true" hidden="true">
		<Size>
			<AbsDimension x="12" y="12"/>
		</Size>
		<Scripts>
			<OnLoad>
				getglobal(this:GetName().."Border"):SetWidth(13);
				getglobal(this:GetName().."Border"):SetHeight(13);
			</OnLoad>
			<OnEnter>
				ER_RaidPulloutButton_OnEnter();
			</OnEnter>
			<OnLeave>
				GameTooltip:Hide();
			</OnLeave>
		</Scripts>
	</Frame>

	<Frame name="ER_RaidPulloutButtonTemplate" hidden="true" virtual="true">
		<Frames>
			<Frame name="$parentDebuff5" inherits="ER_RaidBuffButtonTemplate" id="5"/>
