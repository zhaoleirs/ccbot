<Ui xmlns="http://www.blizzard.com/wow/ui/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.blizzard.com/wow/ui/">
    <Script file="EnergyTick.lua"/>
    <Frame name="EnergyTick_Frame" hidden="true">
        <Scripts>
            <OnLoad>
				ET_OnLoad();
			</OnLoad>
			<OnEvent>
				ET_OnEvent(event,arg1);
			</OnEvent>
        </Scripts>
    </Frame>
    <Frame name="ET_Update_Frame" hidden="false">
		<Scripts>
			<OnUpdate>
				ET_OnUpdate(arg1);
			</OnUpdate>
        </Scripts>
    </Frame>
</Ui>