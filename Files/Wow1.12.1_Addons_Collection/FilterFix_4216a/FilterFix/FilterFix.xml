<Ui xmlns="http://www.blizzard.com/wow/ui/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.blizzard.com/wow/ui/ ..\FrameXML\UI.xsd">
	<Script file="localization.lua"/> 
	<Script file="FilterFix.lua"/> 
	
	<Frame name="FilterFixFrame" hidden="true">
		<Scripts>
			<OnLoad>
				FilterFix_OnLoad();
			</OnLoad>
			<OnEvent>
				FilterFix_OnEvent(event);
			</OnEvent>
		</Scripts>
	</Frame>
	<GameTooltip name="FilterFixTooltip" inherits="GameTooltipTemplate" parent="UIParent" hidden="true"/>
</Ui>