<Bindings>
	<Binding name="FLEXBAR_MAINMENU" runOnUp="true" header="FLEXBAR_SCRIPTS">
		if ( keystate == "up" ) then
			if FBMenu1:IsVisible() then
				FBMenu1:Hide()
			else
				FB_GUI_Main_Menu()
			end
		end
	</Binding>
	<Binding name="FLEXBAR_AUTOITEM" runOnUp="true">
		if ( keystate == "up" ) then
			if FBAutoItemsFrame:IsVisible() then
				HideUIPanel(FBAutoItemsFrame)
			else
				FB_DisplayAutoItems()
			end
		end
	</Binding>
	<Binding name="FLEXBAR_SCRIPTS" runOnUp="true">
		if ( keystate == "up" ) then
			if FBScriptsFrame:IsVisible() then
				FBScriptsEditBox:ClearFocus()
				FBScriptsFrame:Hide()
			else
				FBScriptsFrame:Show()
				FBScriptsEditBox:SetFocus()
			end
		end
	</Binding>
	<Binding name="FLEXBAR_PERFORMANCE" runOnUp="true">
		if ( keystate == "up" ) then
			if FBPerformanceFrame:IsVisible() then
				HideUIPanel(FBPerformanceFrame)
			else
				FB_Show_PerformanceOptions()
			end
		end
	</Binding>
	<Binding name="FLEXBAR_OPTIONS" runOnUp="true">
		if ( keystate == "up" ) then
			if FBOptionsFrame:IsVisible() then
				HideUIPanel(FBOptionsFrame)
			else
				FB_ShowGlobalOptions()
			end
		end
	</Binding>
	<Binding name="FLEXBAR_EVENTED" runOnUp="true">
		if ( keystate == "up" ) then
			if FBEventEditorFrame:IsVisible() then
				HideUIPanel(FBEventEditorFrame)
			else
				FB_DisplayEventEditor()
			end
		end
	</Binding>
	<Binding name="FLEXBAR_BUTNINFO" runOnUp="true">
		if ( keystate == "up" ) then
			if FBButtonInfoShown then
				FB_RestoreButtonText()
			else
				FB_ShowButtonInfo()
			end
		end
	</Binding>
	<Binding name="FLEXBAR_BUTNTOOLTIP" runOnUp="true">
		if ( keystate == "up" ) then
			if FBButtonInfoTooltipShown then
				FBButtonInfoTooltipShown = false
			else
				FBButtonInfoTooltipShown = true
			end
		end
	</Binding>
