	</Binding>
	<Binding name="CONJURE_FOOD">
			ConjureFu:Conjure("food")
	</Binding>
	<Binding name="CONJURE_GEM">
			ConjureFu:Conjure("gem")
	</Binding>
	<Binding name="CONSUME_WATER">
			ConjureFu:Consume("water")
	</Binding>
	<Binding name="CONSUME_FOOD">
			ConjureFu:Consume("food")
