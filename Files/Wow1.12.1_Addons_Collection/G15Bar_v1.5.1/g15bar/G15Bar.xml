  <Script file="G15Bar.lua"/>
  <CheckButton name="G15BarButton" inherits="ActionButtonTemplate" virtual="true">
    <Scripts>
      <OnLoad>
        G15BarButton_OnLoad();
      </OnLoad>
      <OnShow>
        G15BarButton_Update();
      </OnShow>
      <OnEvent>
        G15BarButton_OnEvent(event);
      </OnEvent>
      <OnClick>
        if ( IsShiftKeyDown() ) then
          PickupAction(G15BarButton_GetID(this));
        else
          if( IsAltKeyDown() ) then
            UseAction(G15BarButton_GetID(this), 1, 1);
          else
            UseAction(G15BarButton_GetID(this), 1);
          end
        end
        G15BarButton_UpdateState();
      </OnClick>
	<OnDragStart>
		if ( LOCK_ACTIONBAR ~= "1" ) then
			PickupAction(G15BarButton_GetID(this));
			ActionButton_UpdateState();
		end
	</OnDragStart>
	<OnReceiveDrag>
		if ( LOCK_ACTIONBAR ~= "1" ) then
			PlaceAction(G15BarButton_GetID(this));
			ActionButton_UpdateState();
		end
	</OnReceiveDrag>
      <OnEnter>
        G15BarButton_SetTooltip();
      </OnEnter>
      <OnLeave>
        this.updateTooltip = nil;
        GameTooltip:Hide();
      </OnLeave>
      <OnUpdate>
        G15BarButton_OnUpdate(arg1);
      </OnUpdate>
    </Scripts>
  </CheckButton>

  <Frame name="G15GrabBarTemplate" virtual="true" hidden="true" movable="true">
		<Backdrop bgFile="Interface\TutorialFrame\TutorialFrameBackground" edgeFile="Interface\DialogFrame\UI-DialogBox-Border" tile="true">
			<EdgeSize>
				<AbsValue val="16"/>
			</EdgeSize>
			<TileSize>
				<AbsValue val="32"/>
			</TileSize>
			<BackgroundInsets>
				<AbsInset left="5" right="5" top="5" bottom="5"/>
			</BackgroundInsets>
		</Backdrop>
		
		<Size>
			<AbsDimension x="60" y="40"/>
		</Size>
		
		<Scripts>
			<OnEnter>
				if((G15BarConfig[G15BarRealm][G15BarChar].bound == false or this:GetName() == "M1GrabBar") and G15BarConfig[G15BarRealm][G15BarChar].lock == false) then
					GameTooltip:SetOwner(this, "ANCHOR_BOTTOMRIGHT");
					GameTooltip:SetText("Left-click to drag");
					G15Bar_Bind(G15BarConfig[G15BarRealm][G15BarChar].bound);
				end
			</OnEnter>
			<OnLeave>
				GameTooltip:Hide();
			</OnLeave>
			<OnMouseDown>
				if (G15BarConfig[G15BarRealm][G15BarChar].lock == false and arg1 == "LeftButton" and (G15BarConfig[G15BarRealm][G15BarChar].bound == false or this:GetName() == "M1GrabBar")) then
					this:StartMoving();
					
				end
			</OnMouseDown>
			<OnMouseUp>
				this:StopMovingOrSizing();
			</OnMouseUp>
		</Scripts>
	</Frame>

