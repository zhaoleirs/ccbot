					</Anchors>
				</FontString>
			</Layer>
		</Layers>
		
		<Frames>
			<Button name="$parentButtonClose" inherits="OptionsButtonTemplate" text="Close">
				<Anchors>
					<Anchor point="BOTTOMRIGHT">
					<Offset><AbsDimension x="-12" y="16"/></Offset>
					</Anchor>
				</Anchors>
				<Scripts>
					<OnClick> G15BarConfigFrame:Hide(); </OnClick>
				</Scripts>
			</Button>
			
			<Button name="$parentButtonAutoBind" inherits="OptionsButtonTemplate" text="AutoBind">
				<Anchors>
					<Anchor point="TOPRIGHT">
					<Offset><AbsDimension x="-12" y="-30"/></Offset>
					</Anchor>
				</Anchors>
				<Scripts>
					<OnClick>
						keys = {"Q", "W", "E", "A", "S", "D", "R", "T", "Y", "F", "G", "H", "U", "I", "O", "J", "K", "L"};
						modifiers = {"SHIFT", "CTRL", "ALT"};
						for i=1,3 do
							for j=1,18 do
								command = "M"..i.."ACTIONBUTTON"..j;
								key = modifiers[i].."-"..keys[j];
								if(G15Bar_AutoBind(key, command) ~= true) then
									DEFAULT_CHAT_FRAME:AddMessage(key.." was already bound to "..GetBindingAction(key).." cannot be bound to "..command);
								end
							end
						end
						if(not IsShiftKeyDown()) then DEFAULT_CHAT_FRAME:AddMessage("The AutoBind key can be shift-clicked to force all bindings."); end;
						SaveBindings(GetCurrentBindingSet());
					</OnClick>
				</Scripts>
			</Button>
			
			<Button name="$parentButtonResetPosition" inherits="OptionsButtonTemplate" text="Reset Position ">
				<Anchors>
					<Anchor point="BOTTOMLEFT">
					<Offset><AbsDimension x="12" y="16"/></Offset>
					</Anchor>
				</Anchors>
				<Scripts>
					<OnClick> 
						G15Bar_Bind(true);
						getglobal("G15BarConfigFrameCheckButtonBound"):SetChecked(true);
						G15Bar_Reset(); 
					</OnClick>
				</Scripts>
			</Button>
			
			<CheckButton name="$parentCheckButtonLock" inherits="OptionsCheckButtonTemplate">
				<Anchors>
					<Anchor point="TOPLEFT" relativeTo="$parent">
					<Offset><AbsDimension x="20" y="-30"/></Offset>
					</Anchor>
				</Anchors>
				<Scripts>
					<OnLoad> getglobal(this:GetName().."Text"):SetText("Lock"); </OnLoad>
					<OnClick> G15Bar_Lock(this:GetChecked()); </OnClick>
				</Scripts>
			</CheckButton>
			
			<CheckButton name="$parentCheckButtonLabels" inherits="OptionsCheckButtonTemplate">
				<Anchors>
					<Anchor point="TOP" relativeTo="$parentCheckButtonLock" relativePoint="BOTTOM">
					<Offset><AbsDimension x="" y="-5"/></Offset>
					</Anchor>
				</Anchors>
				<Scripts>
					<OnLoad> getglobal(this:GetName().."Text"):SetText("Show Labels"); </OnLoad>
					<OnClick> G15Bar_ShowLabels(this:GetChecked()); </OnClick>
				</Scripts>
			</CheckButton>
			
			<CheckButton name="$parentCheckButtonBound" inherits="OptionsCheckButtonTemplate">
				<Anchors>
					<Anchor point="TOP" relativeTo="$parentCheckButtonLabels" relativePoint="BOTTOM">
					<Offset><AbsDimension x="" y="-5"/></Offset>
					</Anchor>
				</Anchors>
				<Scripts>
					<OnLoad> getglobal(this:GetName().."Text"):SetText("Linked Together M-Bars"); </OnLoad>
					<OnClick>G15Bar_Bind(this:GetChecked());</OnClick>
				</Scripts>
			</CheckButton>
			
			<CheckButton name="$parentCheckButtonGrid" inherits="OptionsCheckButtonTemplate">
				<Anchors>
					<Anchor point="TOP" relativeTo="$parentCheckButtonBound" relativePoint="BOTTOM">
					<Offset><AbsDimension x="" y="-5"/></Offset>
					</Anchor>
				</Anchors>
				<Scripts>
					<OnLoad> getglobal(this:GetName().."Text"):SetText("Show Grid"); </OnLoad>
					<OnClick>G15Bar_ShowGrid(this:GetChecked());</OnClick>
				</Scripts>
			</CheckButton>
			
			<Slider name="$parentSliderNumBars" inherits="OptionsSliderTemplate">
				<Size>
					<AbsDimension x="220" y="16"/>
				</Size>
				<Anchors>
					<Anchor point="TOPLEFT" relativeTo="$parentCheckButtonGrid" relativePoint="BOTTOMLEFT">
				<Offset><AbsDimension x="0" y="-10"/></Offset>
				</Anchor>
				</Anchors>
				<Scripts>
					<OnLoad>
						getglobal(this:GetName().."Text"):SetText("NumBars");
						getglobal(this:GetName().."High"):SetText("3");
						getglobal(this:GetName().."Low"):SetText("0");
						this:SetMinMaxValues(0,3);
						this:SetValueStep(1);
					</OnLoad>
					<OnValueChanged>
						G15Bar_NumBars(this:GetValue());
					</OnValueChanged>
				</Scripts>
			</Slider>
			
			<Slider name="$parentSliderScale" inherits="OptionsSliderTemplate">
				<Size>
					<AbsDimension x="220" y="16"/>
				</Size>
				<Anchors>
					<Anchor point="TOP" relativeTo="$parentSliderNumBars" relativePoint="BOTTOM">
				<Offset><AbsDimension x="0" y="-20"/></Offset>
				</Anchor>
				</Anchors>
				<Scripts>
					<OnLoad>
						getglobal(this:GetName().."Text"):SetText("Scale");
						getglobal(this:GetName().."High"):SetText("200%");
						getglobal(this:GetName().."Low"):SetText("20%");
						this:SetMinMaxValues(2,20);
						this:SetValueStep(1);
					</OnLoad>
					<OnValueChanged>
						if ( this:GetValue()/10 ~= G15BarConfig[G15BarRealm][G15BarChar].scale) then
							G15Bar_Bind(true);
							getglobal("G15BarConfigFrameCheckButtonBound"):SetChecked( true );
							G15Bar_Scale(this:GetValue()/10);
						end
						percent = this:GetValue()*10;
						getglobal(this:GetName().."Text"):SetText("Scale: "..percent.."%");
					</OnValueChanged>
				</Scripts>
			</Slider>
		</Frames>
			
		<Scripts>
		 	<OnShow>
				if ( not G15BarVariablesLoaded ) then 
					this:Hide();
					return;
				end
				 
				getglobal(this:GetName().."CheckButtonLock")	:SetChecked	( G15BarConfig[G15BarRealm][G15BarChar].lock );
				getglobal(this:GetName().."CheckButtonLabels")	:SetChecked	( G15BarConfig[G15BarRealm][G15BarChar].labels );
				getglobal(this:GetName().."CheckButtonGrid")	:SetChecked	( G15BarConfig[G15BarRealm][G15BarChar].grid );
				getglobal(this:GetName().."CheckButtonBound")	:SetChecked	( G15BarConfig[G15BarRealm][G15BarChar].bound );
				getglobal(this:GetName().."SliderScale")		:SetValue	( G15BarConfig[G15BarRealm][G15BarChar].scale * 10 );
				getglobal(this:GetName().."SliderNumBars")		:SetValue	( G15BarConfig[G15BarRealm][G15BarChar].numbars );
			</OnShow>
		</Scripts>
	 </Frame>
	 
