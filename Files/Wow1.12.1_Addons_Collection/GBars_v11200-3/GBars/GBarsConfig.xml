				<OnEnterPressed>
					local scale = this:GetNumber()
					local barname = "GAutoBar"

					GBars_Scale(barname, scale)
				</OnEnterPressed>
				<OnEscapePressed>
					this:ClearFocus()
				</OnEscapePressed>
				<OnLoad>
					this:SetAutoFocus(0)
					this:SetText(GAutoBar:GetScale())
				</OnLoad>
			</Scripts>
			<FontString inherits="ChatFontNormal"></FontString>
		</EditBox>

