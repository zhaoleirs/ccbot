<Ui xmlns="http://www.blizzard.com/wow/ui" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">

	<Button name="GridFrameTemplateSecure" movable="true" inherits="SecureUnitButtonTemplate" hidden="true" virtual="true">
		<Scripts>
			<OnLoad>
				GridFrame_OnLoad(self)
			</OnLoad>
			<OnAttributeChanged>
				GridFrame_OnAttributeChanged(self, name, value)
			</OnAttributeChanged>
		</Scripts>
	</Button>

	<Button name="GridFrameTemplate" movable="true" hidden="true" virtual="true">
		<Scripts>
			<OnLoad>
				GridFrame_OnLoad(this)
			</OnLoad>
		</Scripts>
	</Button>
</Ui>
