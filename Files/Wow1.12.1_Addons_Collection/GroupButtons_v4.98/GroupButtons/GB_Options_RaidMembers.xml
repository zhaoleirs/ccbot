<Ui xmlns="http://www.blizzard.com/wow/ui/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.blizzard.com/wow/ui/"> 

<CheckButton name="GB_RaidMemberTemplate" inherits="UICheckButtonTemplate" virtual="true">
	<Size><AbsDimension x="24" y="24"/></Size>
	<Layers>
		<Layer level="ARTWORK">
			<FontString name="$parent_Label" inherits="GameFontNormal" justifyH="LEFT">
				<Color r="1.0" g="1.0" b="1.0"/>
				<Size><AbsDimension x="250" y="20"/></Size>
				<Anchors>
					<Anchor point="LEFT" relativeTo="$parent" relativePoint="RIGHT">
						<Offset><AbsDimension x="5" y="1"/></Offset>
					</Anchor>
				</Anchors>
			</FontString>
		</Layer>
	</Layers>
	<Scripts>
		<OnClick>
			GB_RaidMemberSelect_OnClick();
		</OnClick>
	</Scripts>
</CheckButton>

<Frame name="GB_RaidMemberSelect" parent="GB_Options" inherits="GB_BGTemplate" hidden="true">
