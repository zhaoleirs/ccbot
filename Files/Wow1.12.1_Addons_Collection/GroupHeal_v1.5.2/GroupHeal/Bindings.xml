	<Binding name="GROUPHEAL_PARTY4_BIGHEAL">
		GroupHeal_CastSpell('party4', 1)
	</Binding>
	<Binding name="GROUPHEAL_TARGET_BIGHEAL">
		if ( GroupHeal.TargetButtons[1] ) then
			GroupHeal_CastSpell(GroupHeal.TargetButtons[1].target, 1)
		end
	</Binding>

