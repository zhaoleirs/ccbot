<Bindings>
	<Binding name="GROUPHEAL_PLAYER_BIGHEAL" header="GROUPHEAL_BIGHEAL">
		GroupHeal_CastSpell('player', 1)
	</Binding>
	<Binding name="GROUPHEAL_PARTY1_BIGHEAL">
		GroupHeal_CastSpell('party1', 1)
	</Binding>
