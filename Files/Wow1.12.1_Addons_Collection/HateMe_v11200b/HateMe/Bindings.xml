<Bindings>
	<Binding name="DOHATE" header="HateMe">
		HateMe_FeelTheHate("HateMe");
	</Binding>
	<Binding name="DOHURT" header="HateMe">
		HateMe_FeelTheHate("HateYou");
	</Binding>
	<Binding name="HateMe Settings" header="HateMe">
		if (HateMeConfig:IsShown()) then
			HateMeConfig:Hide();
		else
			HateMeConfig:Show();
		end
	</Binding>
</Bindings>
