<Ui xmlns="http://www.blizzard.com/wow/ui/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.blizzard.com/wow/ui/..\FrameXML\UI.xsd">

<Button name="HolyHopeSCommandButton" hidden="false" enableMouse="true"  toplevel="true" parent="UIParent" movable="false" inherits="CooldownCount">
		<Size>
			<AbsDimension x="40" y="40"/>
		</Size>
		<Anchors>
			<Anchor point="CENTER" relativeTo="HolyHopeButton2" relativePoint="CENTER">
				<Offset>
					<AbsDimension x="17" y="-100"/>
				</Offset>
			</Anchor>
		</Anchors>
		<Layers>
			<Layer level="ARTWORK">
				<FontString name="Cooldown15" inherits="GameFontNormal">
					<Color r="1.0" g="1.0" b="0.0" a="1"/>
					<Anchors>
						<Anchor point="CENTER" relativeTo="HolyHopeSCommandButton" relativeFrom="CENTER">
							<Offset>
								<AbsDimension x="0" y="0"/>
							</Offset>
						</Anchor>
					</Anchors>
				</FontString>
			</Layer>
		</Layers>
		<Scripts>
	   	<OnEnter>
		    HolyHope_BuildTooltip(this,"ANCHOR_CURSOR","SCommand");
		  </OnEnter>
      <OnLeave>
				GameTooltip:Hide();
			</OnLeave>
			<OnLoad>
				this:RegisterForDrag("LeftButton");
				this:RegisterForClicks("LeftButtonUp", "RightButtonUp");
			</OnLoad>
			<OnClick>
			  HolyHope_CastSeal("Command",arg1);
			</OnClick>
		</Scripts>
		<NormalTexture file="Interface\AddOns\HolyHope\UI\SCommand"/>
		<HighlightTexture file="Interface\AddOns\HolyHope\UI\SCommand2"/>
	</Button>
	
