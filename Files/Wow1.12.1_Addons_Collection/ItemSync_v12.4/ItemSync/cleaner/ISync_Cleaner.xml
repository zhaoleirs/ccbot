<Ui xmlns="http://www.blizzard.com/wow/ui/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.blizzard.com/wow/ui/
	..\FrameXML\UI.xsd">
    	<Script file="ISync_Cleaner.lua"/>
	

   	<Frame name="ISync_Cleaner_Timer">
		<Scripts>
			<OnLoad>
		        	if(ISync) then ISync:Cleaner_Load(); end
		      </OnLoad>
			<OnUpdate>
				if(ISync) then ISync:Cleaner_Update(); end
			</OnUpdate>
		</Scripts>
	</Frame>
	
	
	<Frame name="ISync_Cleaner_Bar" parent="UIParent" hidden="true" movable="true">
		<Size>
			<AbsDimension x="206" y="26"/>
		</Size>
		<Anchors>
			<Anchor point="CENTER">
				<Offset>
					<AbsDimension x="0" y="80"/>
				</Offset>
			</Anchor>
		</Anchors>
		<Layers>
			<Layer level="BACKGROUND">
				<Texture>
					<Size>
						<AbsDimension x="195" y="13"/>
					</Size>
					<Anchors>
						<Anchor point="TOP">
							<Offset>
								<AbsDimension x="0" y="-2"/>
							</Offset>
						</Anchor>
					</Anchors>
					<Color r="0" g="0" b="0" a="0.5"/>
				</Texture>		
			</Layer>
			<Layer level="OVERLAY">
				<FontString name="ISync_Cleaner_BarText" inherits="GameFontHighlight">
					<Size>
						<AbsDimension x="185" y="16"/>
					</Size>
					<Anchors>
						<Anchor point="TOP">
							<Offset>
								<AbsDimension x="0" y="0"/>
							</Offset>
						</Anchor>
					</Anchors>
				</FontString>
				<FontString name="ISync_Cleaner_BarText2" inherits="GameFontHighlight" text="ISYNC_CLEANER_HEADER">
					<Size>
						<AbsDimension x="185" y="16"/>
					</Size>
					<Anchors>
						<Anchor point="TOP">
							<Offset>
								<AbsDimension x="0" y="18"/>
							</Offset>
						</Anchor>
					</Anchors>
				</FontString>
				<Texture file="Interface\Tooltips\UI-StatusBar-Border">
					<Size>
						<AbsDimension x="205" y="20"/>
					</Size>
					<Anchors>
						<Anchor point="TOP">
							<Offset>
								<AbsDimension x="0" y="2"/>
							</Offset>
						</Anchor>
					</Anchors>
				</Texture>
			</Layer>
		</Layers>
		<Frames>
			<StatusBar name="ISync_Cleaner_BarFrameStatusBar">
				<Size>
					<AbsDimension x="195" y="13"/>
				</Size>
				<Anchors>
					<Anchor point="TOP">
						<Offset>
							<AbsDimension x="0" y="-2"/>
						</Offset>
					</Anchor>
				</Anchors>
				<Layers>
					<Layer level="OVERLAY">

					</Layer>
				</Layers>
				<Scripts>
					<OnLoad>
						this:SetFrameLevel(this:GetFrameLevel() - 1)
					</OnLoad>
				</Scripts>
				<BarTexture file="Interface\TargetingFrame\UI-StatusBar"/>
				<BarColor r="1.0" g="0.0" b="0.0"/>
			</StatusBar>
		</Frames>
		
 			<Scripts>
		     		<OnMouseDown> this:StartMoving();  </OnMouseDown>
		      		<OnMouseUp>  this:StopMovingOrSizing(); </OnMouseUp>
		      		<OnDragStop> this:StopMovingOrSizing(); </OnDragStop>
 			</Scripts>
	</Frame>
	
	
</Ui>
