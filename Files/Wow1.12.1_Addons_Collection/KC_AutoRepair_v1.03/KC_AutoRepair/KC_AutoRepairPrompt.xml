<Ui xmlns="http://www.blizzard.com/wow/ui/"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.blizzard.com/wow/ui/FrameXML/UI.xsd">

    <Frame name="KC_AutoRepairPrompt" inherits="AceGUIDialogTemplate" hidden="true" parent="UIParent">
        <Frames>
			<Frame name="$parentFrame" inherits="AceGUIOptionsBoxTemplate">
				<Layers>
					<Layer level="ARTWORK">
						<FontString name="$parentLine1L" inherits="GameFontNormal"/>
						<FontString name="$parentLine2L" inherits="GameFontNormal"/>
						<FontString name="$parentLine3L" inherits="GameFontNormal"/>
						<FontString name="$parentLine1R" inherits="GameFontNormal"/>
						<FontString name="$parentLine2R" inherits="GameFontNormal"/>
						<FontString name="$parentLine3R" inherits="GameFontNormal"/>
					</Layer>
				</Layers>
				<Frames>
                </Frames>
            </Frame>
			<Button name="$parentInventory" inherits="AceGUIPanelButtonTemplate"/>
			<Button name="$parentEquipment" inherits="AceGUIPanelButtonTemplate"/>
			<Button name="$parentBoth" inherits="AceGUIPanelButtonTemplate"/>
        </Frames>
    </Frame>
</Ui>
