<Bindings>
	<Binding name="MainAssist SmartAssist" header="MAINASSISTHEADER">
		MainAssist:SmartAssist()
	</Binding>
	<Binding name="MainAssist Options" header="MAINASSISTHEADER">
		MainAssist:ShowMenu()
	</Binding>
	<Binding name="MainAssist Add Target to Custom Tank List" header="MAINASSISTHEADER">
		MainAssist:AddCustomTankFromTarget()
	</Binding>
	<Binding name="MainAssist Clear Custom Tank List" header="MAINASSISTHEADER">
		MainAssist:ClearCustomTanks()
	</Binding>
</Bindings>