				<Scripts>
					<OnLoad>
						TitleWideEditBox:SetText("");
					</OnLoad>
					<OnTextChanged>
						if (TitleWideEditBox:GetText() == "" or TitleWideEditBox:GetText() == nil) then
							MapNotesButtonSaveNote:Disable();
						else
							MapNotesButtonSaveNote:Enable();
						end
					</OnTextChanged>
					<OnEnterPressed>
						PlaySound("igMainMenuOption");
						MapNotes_WriteNote();
					</OnEnterPressed>
