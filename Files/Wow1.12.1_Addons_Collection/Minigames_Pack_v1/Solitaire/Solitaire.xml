	<Button name="Solitaire_CardButtonTemplate" virtual="true">
		<Size>
			<AbsDimension x="64" y="90"/>
		</Size>

		<Layers>
			<Layer level="ARTWORK">
				<Texture name="$ParentCardTexture" file="Interface\AddOns\Solitaire\Cardfacev3">
					<Size>
						<AbsDimension x="64" y="90"/>
					</Size>
					<Anchors>
						<Anchor point="TOPLEFT">
							<Offset>
								<AbsDimension x="0" y="0"/>
							</Offset>
						</Anchor>
					</Anchors>
					 
				</Texture>

				<FontString name="$ParentCardValue" inherits="SolitaireFont">
					<FontHeight>
						<AbsValue val="1" /> 
					</FontHeight>

					<Anchors>
						<Anchor point="TOPLEFT">
							<Offset>
								<AbsDimension x="2" y="0"/>
							</Offset>
						</Anchor>
					</Anchors>
					<Color r="0" g="0" b="0" a="1"/>
					
				</FontString>

			</Layer> 

			<Layer level = "OVERLAY">
				<Texture name="$ParentSuitTexture">
					<Size>
						<AbsDimension x="16" y="16"/>
					</Size>
					<Anchors>
						<Anchor point="TOP">
							<Offset>
								<AbsDimension x="0" y="-2"/>
							</Offset>
						</Anchor>
					</Anchors>
				</Texture>

				<Texture name="$ParentCheckTexture">
					<Size>
						<AbsDimension x="16" y="16"/>
					</Size>
					<Anchors>
						<Anchor point="TOPRIGHT">
							<Offset>
								<AbsDimension x="-3" y="0"/>
							</Offset>
						</Anchor>
					</Anchors>
				</Texture>
			</Layer>
		</Layers>

		<Scripts>
			<OnClick>
				--Solitaire_OnClick(this:GetID());
			</OnClick>

			<OnMouseDown>
				Solitaire_SetDragCard(this:GetID());
			</OnMouseDown>
			
			<OnMouseUp>
				Solitaire_PlaceDragCard();
			</OnMouseUp>

			<OnEnter>
				DestinationCard = this:GetID();
			</OnEnter>

			<OnLeave>
				DestinationCard = nil;
			</OnLeave>
		</Scripts>

	</Button>


