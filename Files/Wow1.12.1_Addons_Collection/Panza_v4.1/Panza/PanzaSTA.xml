				<Anchors>
					<Anchor point="BOTTOMLEFT" relativePoint="BOTTOMLEFT" relativeTo="PanzaSTAFrame">
						<Offset>
							<AbsDimension x="24" y="30"/>
						</Offset>
					</Anchor>
				</Anchors>
				<Scripts>
					<OnClick>
						PA:STA_btnDone_OnClick()
						if (PanzaTreeFrame:IsVisible()) then
							PanzaTreeFrame:Hide();
						end
					</OnClick>
				</Scripts>
			</Button>

		</Frames>
		<Scripts>
			<OnLoad>
				PA:STA_OnLoad();
				getglobal(this:GetName().."Title"):SetText(PANZA_STA_TITLE);
			</OnLoad>
			<OnShow>
				this:SetBackdropColor(0, 0, 0);
				PA:STA_OnShow();
			</OnShow>
			<OnHide>
