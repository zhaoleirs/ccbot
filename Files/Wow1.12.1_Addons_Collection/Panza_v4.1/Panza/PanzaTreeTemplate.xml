					<Anchor point="TOP" relativeTo="$parentTitle3" relativePoint="BOTTOM"/>
				</Anchors>
			</Frame>
			<Frame name="$parentTitle5" inherits="EarthTreeButtonFrameTemplate" id="5" hidden="true">
