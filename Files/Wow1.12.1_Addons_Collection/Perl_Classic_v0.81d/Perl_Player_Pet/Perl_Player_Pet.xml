			<AbsDimension x="170" y="24"/>
		</Size>
		<Anchors>
			<Anchor point="TOPLEFT">
				<Offset>
					<AbsDimension x="27" y="-112"/>
				</Offset>
			</Anchor>
		</Anchors>

