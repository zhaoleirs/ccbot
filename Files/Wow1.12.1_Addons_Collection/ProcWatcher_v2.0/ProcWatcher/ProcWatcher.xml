<Frames>
		<Button name="ProcWatcherCloseButton">
			<Size>
				<AbsDimension x="16" y="16"/>
			</Size>
			<Anchors>
				<Anchor point="TOPRIGHT">
					<Offset>
						<AbsDimension x="-8" y="-8"/>
					</Offset>
				</Anchor>
			</Anchors>
			<Scripts>
				<OnClick>
					HideUIPanel(ProcWatcherFrame);
					DEFAULT_CHAT_FRAME:AddMessage("|cffff9999ProcWatcher window closed|cffffffff - data still being collected.");

				</OnClick>
			</Scripts>
			<NormalTexture file="Interface\AddOns\ProcWatcher\CloseButton-Up"/>
			<PushedTexture file="Interface\AddOns\ProcWatcher\CloseButton-Down"/>
			<HighlightTexture file="Interface\AddOns\ProcWatcher\Idle" alphaMode="ADD"/>
		</Button>

</Frames>

-----------------------------------------------------------------------------------------------

<Scripts>

	<OnHide>
		ProcWatcherFrame:StopMovingOrSizing();
	</OnHide>
	<OnDragStart>
		ProcWatcherFrame:StartMoving();
	</OnDragStart>
	<OnDragStop>
		ProcWatcherFrame:StopMovingOrSizing();
	</OnDragStop>
	<OnMouseUp>
		ProcWatcherFrame:StopMovingOrSizing();
	</OnMouseUp>
	<OnLoad>
		ProcWatcher_OnLoad();
	</OnLoad>

        <OnEvent>
		ProcWatcher_OnEvent(event,arg1,arg2,arg3,arg4,arg5);
        </OnEvent>

</Scripts>

</Frame>

