<Ui xmlns="http://www.blizzard.com/wow/ui/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.blizzard.com/wow/ui/
..\FrameXML\UI.xsd">
	<!-- Hidden tooltip used for info harvesting -->
	<GameTooltip name="IsMountedTooltip" frameStrata="TOOLTIP" hidden="true" inherits="GameTooltipTemplate" parent="UIParent" />
	<Script file="IsMounted.lua"/>
	<Frame name="IsMountedFrame" hidden="true">
		<Scripts>
			<OnLoad>
				IsMounted.OnLoad();
            </OnLoad>
            <OnEvent>
				IsMounted.OnEvent(event);
            </OnEvent>
		</Scripts>
	</Frame>
</Ui>
