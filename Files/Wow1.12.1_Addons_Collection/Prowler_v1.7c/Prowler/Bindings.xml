	</Binding>

	<Binding name="Prowler_Human">
		Shapeshift(Human)
	</Binding>

	<Binding name="Prowler_Travel">
		Shapeshift(Travel)
	</Binding>

	<Binding name="Prowler_Best">
		Prowler_BestForm()
	</Binding>	

</Bindings>