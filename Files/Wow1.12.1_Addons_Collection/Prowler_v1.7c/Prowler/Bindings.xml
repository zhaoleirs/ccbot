	</Binding>
	
	<Binding name="Prowler_Bear">
		Prowler_Bear()
	</Binding>

	<Binding name="Prowler_Prowl">
		Prowler_Prowl()
	</Binding>
	
	<Binding name="Prowler_Moon">
