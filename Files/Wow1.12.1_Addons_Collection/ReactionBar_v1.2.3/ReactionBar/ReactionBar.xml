<Ui xmlns="http://www.blizzard.com/wow/ui/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.blizzard.com/wow/ui/
..\FrameXML\UI.xsd">
	<Script file="ReactionBar.lua"/>
	<Script file="localization.lua"/>
	<Frame name="ReactionBarFrame">
		<Scripts>
			<OnLoad>
			  ReactionBar_OnLoad()
			</OnLoad>
			<OnEvent>
				ReactionBar_OnEvent(event)
			</OnEvent>
		</Scripts>
	</Frame>
	<Frame name="ReactionBarHunterFrame" hidden="true">
		<Scripts>
			<OnUpdate>
				ReactionBar_HunterOnUpdate(arg1)
			</OnUpdate>
		</Scripts>
	</Frame>
	<Frame name="ReactionBarMetaFrame" hidden="true">
		<Scripts>
			<OnUpdate>
				ReactionBar_MetaOnUpdate(arg1)
			</OnUpdate>
		</Scripts>
	</Frame>
</Ui>