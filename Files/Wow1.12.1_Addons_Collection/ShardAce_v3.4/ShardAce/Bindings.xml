<Bindings>
	<Binding name="SORT" header="SHARDACE">
		ShardAce:CountClick("LeftButton")
	</Binding>
	<Binding name="HEALTH">
		ShardAce:HealthClick("LeftButton")
	</Binding>
	<Binding name="SOULSTONE">
		ShardAce:SoulClick("LeftButton")
	</Binding>
	<Binding name="MOUNT">
		ShardAce:SoulClick("RightButton")
	</Binding>
	<Binding name="SPELLSTONE">
		ShardAce:StoneClick("LeftButton")
	</Binding>
	<Binding name="FIRESTONE">
		ShardAce:StoneClick("RightButton")
	</Binding>
	<Binding name="BUFF">
		ShardAceEngine:CastSpell("DemonBuff")
	</Binding>
	<Binding name="SUMMONER">
		ShardAce:HealthClick("RightButton")
	</Binding>
</Bindings>
