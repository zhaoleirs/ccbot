<Bindings>
	<Binding name="DOSHARDTRACKERSORT" header="SHARDTRACKER">
		ShardTracker_SortShards("LeftButton");
	</Binding>
	<Binding name="DOSHARDTRACKERHEALTH">
		ShardTracker_OnHealthButtonClick("LeftButton");
	</Binding>
	<Binding name="DOSHARDTRACKERSOUL">
		ShardTracker_OnSoulButtonClick("LeftButton");
	</Binding>
	<Binding name="DOSHARDTRACKERMOUNT">
		ShardTracker_OnSoulButtonClick("RightButton");
	</Binding>
	<Binding name="DOSHARDTRACKERSPELL">
		ShardTracker_OnSpellButtonClick("LeftButton");
	</Binding>
	<Binding name="DOSHARDTRACKERFIRE">
		ShardTracker_OnSpellButtonClick("RightButton");
	</Binding>
	<Binding name="DOSHARDTRACKERBUFF">
		CastBuff();
	</Binding>
	<Binding name="DOSHARDTRACKERSUMMONER">
		ShardTracker_OnHealthButtonClick("RightButton");
	</Binding>
</Bindings>
