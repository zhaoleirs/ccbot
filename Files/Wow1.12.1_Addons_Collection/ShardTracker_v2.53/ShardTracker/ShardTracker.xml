	<Button name="STTimerButton" parent="Minimap" toplevel="true" hidden="true" frameStrata="LOW" enableMouse="true" movable="true">	
		<NormalTexture file="Interface\AddOns\ShardTracker\Images\SpellTimerButton"/>
		<HighlightTexture alphaMode="ADD" file="Interface\AddOns\ShardTracker\Images\SpellTimerButton"/>
		<Size>
			<AbsDimension x="30" y="30"/>
		</Size>
        <Anchors>
            <Anchor point="BOTTOM" relativeTo="Minimap">
                <Offset>
                    <AbsDimension x="-150" y="100"/>
                </Offset>
            </Anchor>
        </Anchors>
		<Scripts>
		    <OnLoad>
        		ShardTracker_Button_OnLoad(this);
			</OnLoad>
			<OnMouseUp>
			 if ( this.isMoving ) then
			  this:StopMovingOrSizing();
			  this.isMoving = false;
			 end
			</OnMouseUp>
			<OnMouseDown>
			 if ( ( ( not this.isLocked ) or ( this.isLocked == 0 ) ) and ( arg1 == "LeftButton" ) ) then
			  this:StartMoving();
			  this.isMoving = true;
			 end
			</OnMouseDown>
			<OnHide>
			 if ( this.isMoving ) then
			  this:StopMovingOrSizing();
			  this.isMoving = false;
			 end
			</OnHide>
		</Scripts>
	</Button>
