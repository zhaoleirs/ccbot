<Ui xmlns="http://www.blizzard.com/wow/ui/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.blizzard.com/wow/ui/ ..\FrameXML\UI.xsd">
        <Script file="SmartPetAttack.lua" />
        <Script file="SmartPet.lua" />
        <!-- <Include file="menu.xml"/> -->
        <Frame name="SmartPetAttackFrame" parent="UIParent">
                <Scripts>
                        <OnLoad>SmartPetAttack_OnLoad();</OnLoad>
                        <OnEvent>SmartPetAttack_OnEvent(event, arg1, arg2, arg3);</OnEvent>
                        <OnUpdate></OnUpdate>
                </Scripts>
        </Frame>
	<GameTooltip name="SmartPetTip" frameStrata="TOOLTIP" hidden="true" parent="UIParent" inherits="GameTooltipTemplate">
                <Scripts>
                        <OnLoad>this:SetOwner(SmartPetAttackFrame, "ANCHOR_NONE");</OnLoad>
                </Scripts>
        </GameTooltip>
</Ui>
