<Button name="SpiritSphereMightButton" hidden="false" enableMouse="true"  toplevel="true" parent="UIParent" movable="false" inherits="CooldownCount">
		<Size>
			<AbsDimension x="40" y="40"/>
		</Size>
		<Anchors>
			<Anchor point="CENTER" relativeTo="SpiritSphereButton" relativePoint="CENTER">
				<Offset>
					<AbsDimension x="17" y="-100"/>
				</Offset>
			</Anchor>
		</Anchors>
		<Layers>
			<Layer level="ARTWORK">
				<FontString name="Cooldown2" inherits="GameFontNormal">
					<Color r="1.0" g="1.0" b="0.0" a="1"/>
					<Anchors>
						<Anchor point="CENTER" relativeTo="SpiritSphereMightButton" relativeFrom="CENTER">
							<Offset>
								<AbsDimension x="0" y="0"/>
							</Offset>
						</Anchor>
					</Anchors>
				</FontString>
			</Layer>
		</Layers>
		<Scripts>
	   	<OnEnter>
		    SpiritSphere_BuildTooltip(this,"ANCHOR_CURSOR","Might");
		  </OnEnter>
      <OnLeave>
				GameTooltip:Hide();
			</OnLeave>
			<OnLoad>
				this:RegisterForDrag("LeftButton");
				this:RegisterForClicks("LeftButtonUp", "RightButtonUp");
			</OnLoad>
			<OnClick>
			  SpiritSphere_CastBlessing("Might",arg1);
			</OnClick>
		</Scripts>
		<NormalTexture file="Interface\AddOns\SpiritSphere\UI\Might"/>
		<HighlightTexture file="Interface\AddOns\SpiritSphere\UI\Might2"/>
	</Button>
	
