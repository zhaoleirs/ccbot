<Ui xmlns="http://www.blizzard.com/wow/ui/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.blizzard.com/wow/ui/..\FrameXML\UI.xsd">

<Button name="SpiritSphereSCommandButton" hidden="false" enableMouse="true"  toplevel="true" parent="UIParent" movable="false" inherits="CooldownCount">
		<Size>
			<AbsDimension x="40" y="40"/>
		</Size>
		<Anchors>
			<Anchor point="CENTER" relativeTo="SpiritSphereButton2" relativePoint="CENTER">
				<Offset>
					<AbsDimension x="17" y="-100"/>
				</Offset>
			</Anchor>
		</Anchors>
		<Layers>
			<Layer level="ARTWORK">
				<FontString name="Cooldown15" inherits="GameFontNormal">
					<Color r="1.0" g="1.0" b="0.0" a="1"/>
					<Anchors>
						<Anchor point="CENTER" relativeTo="SpiritSphereSCommandButton" relativeFrom="CENTER">
							<Offset>
								<AbsDimension x="0" y="0"/>
							</Offset>
						</Anchor>
					</Anchors>
				</FontString>
			</Layer>
		</Layers>
		<Scripts>
	   	<OnEnter>
		    SpiritSphere_BuildTooltip(this,"ANCHOR_CURSOR","SCommand");
		  </OnEnter>
      <OnLeave>
				GameTooltip:Hide();
			</OnLeave>
			<OnLoad>
				this:RegisterForDrag("LeftButton");
				this:RegisterForClicks("LeftButtonUp", "RightButtonUp");
			</OnLoad>
			<OnClick>
			  SpiritSphere_CastSeal("Command",arg1);
			</OnClick>
		</Scripts>
		<NormalTexture file="Interface\AddOns\SpiritSphere\UI\SCommand"/>
		<HighlightTexture file="Interface\AddOns\SpiritSphere\UI\SCommand2"/>
	</Button>
	
