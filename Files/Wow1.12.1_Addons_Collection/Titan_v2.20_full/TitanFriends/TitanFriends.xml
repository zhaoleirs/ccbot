<Ui xmlns="http://www.blizzard.com/wow/ui/"
 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
 xsi:schemaLocation="http://www.blizzard.com/wow/ui/ ..\FrameXML\UI.xsd"> 
	<Script file="localization.lua"/>
	<Script file="TitanFriends.lua"/>
	<Frame parent="UIParent">
		<Frames>
			<Button name="TitanPanelFriendsButton" inherits="TitanPanelComboTemplate" frameStrata="FULLSCREEN" toplevel="true">
				<Scripts>
					<OnLoad>
						TitanPanelFriendsButton_OnLoad();
						TitanPanelButton_OnLoad();
					</OnLoad>
					<OnEvent>
						TitanPanelFriendsButton_OnEvent(event, arg1, arg2, arg3);
					</OnEvent>
					<OnEnter>
						TitanPanelFriendsButton_OnEnter();
						TitanPanelButton_OnEnter();
					</OnEnter>
					<OnClick>
						TitanPanelFriendsButton_OnClick();
					</OnClick>
				</Scripts>
			</Button>
		</Frames>
	</Frame>

	
</Ui>