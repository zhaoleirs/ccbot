			<Button name="TitanPanelTitanHawkButton" inherits="TitanPanelButtonTemplate" frameStrata="FULLSCREEN" toplevel="true">
				<Size>
		                	<AbsDimension x="128" y="20"/>
		                </Size>
				<Scripts>
					<OnLoad>
						TitanPanelTitanHawkButton_OnLoad();
						TitanPanelButton_OnLoad();
					</OnLoad>
					<OnShow>
						TitanPanelTitanHawkButton_OnShow();
						
					</OnShow>
					<OnHide>
						TitanPanelTitanHawkButton_OnHide();
						
					</OnHide>

				</Scripts>
			</Button>
			<Frame name="SFBTitanHawk_DropDown" inherits="UIDropDownMenuTemplate" hidden="true">
				<Size>
					<AbsDimension x="1" y="1"/>
				</Size>
				<Anchors>
					<Anchor point="TOPLEFT">
						<Offset>
							<AbsDimension x="0" y="0"/>
						</Offset>
					</Anchor>
				</Anchors>
				<Scripts>
					<OnLoad>
						SFBTitanHawk_DropDown_OnLoad();
					</OnLoad>
				</Scripts>
			</Frame>
			
		</Frames>
	</Frame>		
</Ui>