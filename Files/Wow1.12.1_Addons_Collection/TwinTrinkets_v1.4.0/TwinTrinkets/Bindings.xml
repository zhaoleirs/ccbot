<Bindings>
	<Binding name="JT_TRINKETAPANDPOM" description="Single-keystroke ownage." header="JT_TWINTRINKETS">
		Trinket_UseTrinket(1,2,1);
	</Binding>
	<Binding name="JT_TRINKETAPPOM" description="Tap once for powerup, twice for PoM">
		Trinket_UseTrinket(1,1,1);
	</Binding>
	<Binding name="JT_TRINKETPOM" description="Use a trinket + pom.">
		Trinket_UseTrinket(1,2,0);
	</Binding>
	<Binding name="JT_TRINKETORPOM" description="Tap once for trinket, twice for pom.">
		Trinket_UseTrinket(1,1,0);
	</Binding>
	<Binding name="JT_TRINKET" description="Use a trinket only.">
		Trinket_UseTrinket(1,0,0);
	</Binding>
</Bindings>