<!--
  CleanMinimap key bindings
  
  $Id: Bindings.xml 55 2005-08-15 18:06:16Z joev $
-->
<Bindings>
  <Binding name="CLEANMINIMAPTOGGLE" header="CLEANMINIMAP">
        CleanMinimap_Toggle();
  </Binding>

  <Binding name="CLEANMINIMAPOPTIONS" header="CLEANMINIMAP">
        CleanMinimapOptions_Toggle();
  </Binding>
  <Binding name="CLEANMINIMAPINCREASEOPACITY" header="CLEANMINIMAP">
        CleanMinimap_IncreaseOpacity();
  </Binding>
  <Binding name="CLEANMINIMAPDECREASEOPACITY" header="CLEANMINIMAP">
        CleanMinimap_DecreaseOpacity();
  </Binding>
  <Binding name="CLEANMINIMAPTOGGLESIZE" header="CLEANMINIMAP">
        CleanMinimap_ToggleSize();
  </Binding>

</Bindings>