<Bindings>
	<Binding name="METAMAP_TOGGLE" header="METAMAP_TITLE">
		MetaMainMap_Toggle();
	</Binding>
	<Binding name="METAMAP_FSTOGGLE">
		MetaMap_FullScreenToggle();
	</Binding>
	<Binding name="METAMAP_SAVESET">
		MetaMapSaveSet_Toggle();
	</Binding>
	<Binding name = "METAMAP_KB">
		MetaMap_ToggleFrame(MetaKB_MainFrame);
	</Binding>
	<Binding name = "METAMAP_KB_TARGET_UNIT">
		MetaKB_UpdateKeySelectedUnit();
	</Binding>
	<Binding name = "METAMAP_QUICKNOTE">
		MetaMapNotes_QuickNoteShow();
	</Binding>
</Bindings>
