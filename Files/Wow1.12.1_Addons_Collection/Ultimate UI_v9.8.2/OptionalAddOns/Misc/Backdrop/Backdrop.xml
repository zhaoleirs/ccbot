<Ui xmlns="http://www.blizzard.com/wow/ui/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.blizzard.com/wow/ui/..\FrameXML\UI.xsd">
	<Script file="Backdrop.lua"/>
	<Frame name="UUIBackdropFrame1" frameStrata="BACKGROUND" enableMouse="true" movable="true" parent="UIParent" resizable="true"><!--setAllPoints="true" -->
		<Size>
			<AbsDimension x="155" y="75"/>
		</Size>
		<Anchors>
			<Anchor point="TOP">
			</Anchor>
		</Anchors>
		<TitleRegion/>
		<!-- bgFile = "Interface\DialogFrame\UI-DialogBox-Background" edgeFile = "Interface\DialogFrame\UI-DialogBox-Border" -->
		<Backdrop bgFile = "Interface\Tooltips\UI-Tooltip-Background" edgeFile="Interface\Tooltips\UI-Tooltip-Border" tile="true">
			<EdgeSize>
				<AbsValue val="16"/>
			</EdgeSize>
			<!--<TileSize>
				<AbsValue val="24"/>
			</TileSize>-->
			<BackgroundInsets>
				<AbsInset left="5" right="5" top="5" bottom="5"/>
			</BackgroundInsets>
		</Backdrop>
		<Scripts>
			<OnLoad>
				UUIBackdrop_OnLoad();
				UUIMainBackdrop_Onload();
			</OnLoad>
			<OnEvent>
			</OnEvent>
			<OnShow>
			</OnShow>
			<OnHide>
			</OnHide>
			<OnUpdate>		
			</OnUpdate>
			<OnMouseDown>
				Backdrop_OnMouseDown(arg1);
			</OnMouseDown>
			<OnMouseUp>
				Backdrop_OnMouseUp(arg1);
			</OnMouseUp>
		</Scripts>
	</Frame>
	
