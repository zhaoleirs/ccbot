<Bindings>
	<Binding name="WARRIOR_BASIC_1" header="WARRIOR_BASIC">
		WARRIOR.Classes:Cast(WARRIOR.Classes._active);
	</Binding>
	<Binding name="WARRIOR_BASIC_2">
		WARRIOR.Alerts:Cast();
	</Binding>
	<Binding name="WARRIOR_BASIC_3">
		WARRIOR.Classes:Cycle("forward");
	</Binding>
	<Binding name="WARRIOR_BASIC_4">
		WARRIOR.Classes:Cycle("backward");
	</Binding>
	<Binding name="WARRIOR_BASIC_5">
		WARRIOR.Player.Targeting:AssistMostWounded();
	</Binding>
	<Binding name="WARRIOR_BASIC_6">
		WARRIOR.GUI:Toggle();
	</Binding>
	<Binding name="WARRIOR_BASIC_7">
		ConsoleExec("reloadui");
	</Binding>
	<Binding name="WARRIOR_STANCE_1" header="WARRIOR_STANCE">
		WARRIOR.Spells:Cast(W.Spells.BattleStance);
	</Binding>
	<Binding name="WARRIOR_STANCE_2">
		WARRIOR.Spells:Cast(W.Spells.DefensiveStance);
	</Binding>
	<Binding name="WARRIOR_STANCE_3">
		WARRIOR.Spells:Cast(W.Spells.BerserkerStance);
	</Binding>
	<Binding name="WARRIOR_STANCE_4">
		WARRIOR.Player.Stances:Toggle(W.Spells.BattleStance,W.Spells.DefensiveStance);
	</Binding>
	<Binding name="WARRIOR_STANCE_5">
		WARRIOR.Player.Stances:Toggle(W.Spells.BattleStance,W.Spells.BerserkerStance);
	</Binding>
	<Binding name="WARRIOR_STANCE_6">
		WARRIOR.Player.Stances:Toggle(W.Spells.DefensiveStance,W.Spells.BerserkerStance);
	</Binding>
	<Binding name="WARRIOR_STANCE_7">
		WARRIOR.Player.Stances:Cycle("forward");
	</Binding>
	<Binding name="WARRIOR_STANCE_8">
		WARRIOR.Player.Stances:Cycle("backward");
	</Binding>
	<Binding name="WARRIOR_CLASS_1" header="WARRIOR_CLASS">
		WARRIOR.Keybindings:Execute(1);
	</Binding>
	<Binding name="WARRIOR_CLASS_2">WARRIOR.Keybindings:Execute(2);</Binding>
	<Binding name="WARRIOR_CLASS_3">WARRIOR.Keybindings:Execute(3);</Binding>
	<Binding name="WARRIOR_CLASS_4">WARRIOR.Keybindings:Execute(4);</Binding>
	<Binding name="WARRIOR_CLASS_5">WARRIOR.Keybindings:Execute(5);</Binding>
	<Binding name="WARRIOR_CLASS_6">WARRIOR.Keybindings:Execute(6);</Binding>
	<Binding name="WARRIOR_CLASS_7">WARRIOR.Keybindings:Execute(7);</Binding>
	<Binding name="WARRIOR_CLASS_8">WARRIOR.Keybindings:Execute(8);</Binding>
	<Binding name="WARRIOR_CLASS_9">WARRIOR.Keybindings:Execute(9);</Binding>
	<Binding name="WARRIOR_CLASS_10">WARRIOR.Keybindings:Execute(10);</Binding>
	<Binding name="WARRIOR_CLASS_11">WARRIOR.Keybindings:Execute(10);</Binding>
	<Binding name="WARRIOR_CLASS_12">WARRIOR.Keybindings:Execute(10);</Binding>
	<Binding name="WARRIOR_CLASS_13">WARRIOR.Keybindings:Execute(10);</Binding>
	<Binding name="WARRIOR_CLASS_14">WARRIOR.Keybindings:Execute(10);</Binding>
	<Binding name="WARRIOR_CLASS_15">WARRIOR.Keybindings:Execute(10);</Binding>
</Bindings>
