..\..\FrameXML\UI.xsd"> 

<Frame name="WonderRepFrame">
 <Scripts>
  <OnLoad>
   WonderRep_OnLoad();
  </OnLoad>
  <OnEvent>
   WonderRep_OnEvent(event);
  </OnEvent>
  <OnUpdate>
   WonderRep_OnUpdate(arg1);
  </OnUpdate>
 </Scripts>
</Frame>

	<Button name="WonderRepButton" frameStrata="BACKGROUND" toplevel="true" movable="true" parent="UIParent" virtual="true">
		<Size>
			<AbsDimension x="5" y="5"/>
		</Size>
		<Scripts>
			<OnUpdate>
				WonderRep_OnUpdate(arg1);
			</OnUpdate>
		</Scripts>
	</Button>

</Ui> 