					<Anchors>
						<Anchor point="CENTER"/>
					</Anchors>
				</FontString>
			</Layer>
		</Layers>
		<Scripts>
			<OnLoad>
				getglobal(this:GetName().."Label"):SetText(strchar(strbyte("A")+this:GetID()-1));
			</OnLoad>
			<OnClick>
				Yule.tab = this:GetID();
				Yule:UpdateTabs();
			</OnClick>
		</Scripts>
		<NormalTexture/>
		<HighlightTexture file="Interface\Buttons\ButtonHilight-Square" alphaMode="ADD"/>
		<CheckedTexture file="Interface\Buttons\CheckButtonHilight" alphaMode="ADD"/>
	</CheckButton>
	
	<!-- chat frame edit box template -->
	<EditBox name="YuleMenuChatFrameEditBoxTemplate" autoFocus="false" inherits="InputBoxTemplate" virtual="true">
		<Size><AbsDimension x="80" y="32"/></Size>
		<Scripts>
			<OnEnterPressed>
				this:ClearFocus();
			</OnEnterPressed>
			<OnEscapePressed>
				this:ClearFocus();
			</OnEscapePressed>
			<OnEditFocusLost>
				if YuleConfig then
					YuleConfig.frames[this:GetID()].chatframe = tonumber(this:GetText());
					Yule:UpdateTabs();
				end
			</OnEditFocusLost>
		</Scripts>
		<FontString inherits="ChatFontNormal" bytes="256"/>
	</EditBox>

