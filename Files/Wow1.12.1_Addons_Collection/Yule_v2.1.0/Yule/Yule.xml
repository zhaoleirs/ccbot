<Ui xsi:schemaLocation="http://www.blizzard.com/wow/ui/">

	<Script file="Yule.lua"/>

	<!-- core frame -->
	<Frame name="YuleFrame" parent="UIParent" hidden="true">
		<Scripts>
			<OnLoad>
				Yule:OnLoad();
			</OnLoad>
			<OnEvent>
				Yule:OnEvent(event, arg1);
			</OnEvent>
		</Scripts>
	</Frame>

	<!-- Checkbox option template -->
	<CheckButton name="YuleOptionTemplate" inherits="UICheckButtonTemplate" virtual="true">
		<Size>
			<AbsDimension x="24" y="24"/>
		</Size>
		<Scripts>
			<OnLoad>
				this.setting = YuleOptions[this:GetID()].s;
				getglobal(this:GetName().."Text"):SetText(YuleOptions[this:GetID()].t);
			</OnLoad>
			<OnShow>
				if Yule.tab then
					this:SetChecked(YuleConfig.frames[Yule.tab][this.setting]);
				end
			</OnShow>
			<OnClick>
				if Yule.tab then
					YuleConfig.frames[Yule.tab][this.setting] = this:GetChecked();
				end
			</OnClick>
		</Scripts>
	</CheckButton>
	
	<!-- Checkbox generic option template -->
	<CheckButton name="YuleGenericOptionTemplate" inherits="UICheckButtonTemplate" virtual="true">
		<Size>
			<AbsDimension x="24" y="24"/>
		</Size>
		<Scripts>
			<OnLoad>
				this.setting = YuleGenericOptions[this:GetID()].s;
				getglobal(this:GetName().."Text"):SetText(YuleGenericOptions[this:GetID()].t);
			</OnLoad>
			<OnShow>
				this:SetChecked(YuleConfig[this.setting]);
			</OnShow>
			<OnClick>
				YuleConfig[this.setting] = this:GetChecked();
			</OnClick>
		</Scripts>
	</CheckButton>

	<!-- Side tab template -->
	<CheckButton name="YuleChatFrameTabTemplate" virtual="true" hidden="false">
