
<Bindings>
  <Binding name="FRIENDLIST_LOCALE_NAME" description="FRIENDLIST_LOCALE_BINDING_DESCRIPTION" header="FRIENDLIST_LOCALE_HEADER">
    FRIENDLIST_ToggleGUI();
  </Binding>
  <Binding name="FRIENDLIST_LOCALE_ADDFRIEND" description="FRIENDLIST_LOCALE_BINDING_DESCRIPTION" header="FRIENDLIST_LOCALE_HEADER">
    FRIENDLISTStaticPopUpAdd:Show();
  </Binding>
  <Binding name="FRIENDLIST_LOCALE_ADDGUILD" description="FRIENDLIST_LOCALE_BINDING_DESCRIPTION" header="FRIENDLIST_LOCALE_HEADER">
    FRIENDLISTStaticPopUpAddGuild:Show();
  </Binding>
  <Binding name="FRIENDLIST_LOCALE_TOGGLEBOX" description="FRIENDLIST_LOCALE_BINDING_DESCRIPTION" header="FRIENDLIST_LOCALE_HEADER">
    FlagRSPInfo.toggleBox();
    --
    --FlagRSPInfoBoxButton_OnClick("target");
    --FlagRSPInfoBoxCloseButton_OnClick();
    --if FlagRSPInfoBoxButton:GetChecked() == nil then
    --FlagRSP.print("must be checked");
    --FlagRSPInfoBoxButton:SetChecked(0);
    --FlagRSPInfoBoxButton:SetChecked(0);
    --else 
    --FlagRSP.print("must be unchecked");
    --FlagRSPInfoBoxButton:SetChecked(0);
    --end
  </Binding>
</Bindings>
