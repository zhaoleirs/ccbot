<!-- Scrolling Combat Text V0.1 -->
<Ui xmlns="http://www.blizzard.com/wow/ui/"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.blizzard.com/wow/ui/"> 
	
	<Frame name="inCombatFrame" hidden="true">
		<Scripts>
	      	<OnLoad>
	       		inCombat_OnLoad();
	      	</OnLoad>
			<OnEvent>
				inCombat_OnEvent();
			</OnEvent>
		</Scripts>
	</Frame>
</Ui>