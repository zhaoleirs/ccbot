<!-- Scrolling Combat Text V0.1 -->
<Ui xmlns="http://www.blizzard.com/wow/ui/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"   xsi:schemaLocation="http://www.blizzard.com/wow/ui/"> 

	<Script file="sct_killingblows.lua"/>
	
	<Frame name="SCT_KILLINGBLOWS_FRAME" enableMouse="false" parent="UIParent">
		<Scripts>
			<OnLoad>
				if ((SCT) and (ParserLib)) then
					SCT_KillingBlows_OnLoad();
				end
			</OnLoad>
			<OnEvent>
				if ((SCT) and (ParserLib)) then
					SCT_KillingBlows_OnEvent(event);
				end
			</OnEvent>
		</Scripts>
	</Frame>
</Ui>