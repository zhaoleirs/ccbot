				</Anchor>
			</Anchors>
			<Scripts>
				<OnMouseUp>
				 if ( wowChess.isMoving ) then
				  wowChess:StopMovingOrSizing();
				  wowChess.isMoving = false;
				 end
				</OnMouseUp>
				<OnMouseDown>
				 if ( ( ( not wowChess.isLocked ) or ( wowChess.isLocked == 0 ) ) and ( arg1 == "LeftButton" ) ) then
				  wowChess:StartMoving();
				  wowChess.isMoving = true;
				 end
				</OnMouseDown>
			</Scripts>
			<Layers>
